y_{t}^{2} & =\sigma_{t}^{2}\epsilon_{t}^{2}\\
\ln y_{t}^{2} & =\ln\sigma_{t}^{2}+\ln\epsilon_{t}^{2}\\
\ln y_{t}^{2} & =\lambda x_{t}+\eta_{t}+\ln\epsilon_{t}^{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
We want an estimator for 
\begin_inset Formula $\lambda$
\end_inset

 of the form
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\hat{\gamma} & =\frac{\sum_{t=1}^{n}g(y_{t})h(x_{t-j})}{\sum_{t=1}^{n}h(x_{t})h(x_{t-j})}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
So we can use
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
g(y_{t}) & =\ln y_{t}^{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Checking
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
E(x_{t}x_{t-1}) & =E(\frac{1}{2}\xi_{t-1}^{2})=\frac{1}{2}\\
E(\ln y_{t}^{2}x_{t-1}) & =E(\lambda x_{t}\xi_{t-1}+\frac{1}{2}\xi_{t-2}\lambda x_{t})\\
 & =\frac{1}{2}\lambda
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
To get
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\hat{\gamma} & =\frac{\sum_{t=1}^{n}(\ln y_{t}^{2})(x_{t-1})}{\sum_{t=1}^{n}(x_{t})(x_{t-1})}\\
 & \overset{p}{\rightarrow}\frac{E(\ln y_{t}^{2}x_{t-1})}{E(x_{t}x_{t-1})}\\
 & =\frac{E(\ln y_{t}^{2}x_{t-1})}{\frac{1}{2}}\\
 & =\frac{\frac{1}{2}\lambda}{\frac{1}{2}}\\
 & =\lambda
\end{align*}

\end_inset


\end_layout

